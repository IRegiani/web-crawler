{
    "name": "webcrawler",
    "version": "1.0.0",
    "description": "WebCrawler with Mongo",
    "private": true,
    "main": "index.js",
    "scripts": {
        "test": "npm run test:unit",
        "pretest:unit": "npm run lint",
        "test:unit": "jest --config jest.config.js --coverage",
        "test:unit:no-cov": "jest --config jest.config.js",
        "lint": "eslint --config .eslintrc.js --ext .js ./src",
        "start": "node index.js",
        "local": "nodemon index.js",
        "serve": "NODE_ENV=production node index.js",
        "serve-big-memory": "NODE_ENV=production node --max-old-space-size=4096 index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/IRegiani/web-crawler.git"
    },
    "author": "Iago R. Regiani",
    "license": "ISC",
    "engines": {
        "node": ">=16.14"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "compression": "^1.7.4",
        "config": "^3.3.7",
        "cors": "^2.8.5",
        "domhandler": "^4.3.0",
        "domutils": "^2.8.0",
        "express": "^4.17.3",
        "express-http-context": "^1.2.4",
        "helmet": "^5.0.2",
        "htmlparser2": "^7.2.0",
        "http-status-codes": "^2.2.0",
        "mongoose": "^6.2.4",
        "on-finished": "^2.4.1",
        "p-limit": "^3.1.0",
        "signale": "^1.4.0",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "eslint": "^8.10.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-async-await": "*",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^27.5.1",
        "jest-html-reporter": "^3.4.2",
        "nodemon": "^2.0.15"
    }
}
